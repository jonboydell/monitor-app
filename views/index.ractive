<html>
    <head>
        <script src="//cdnjs.cloudflare.com/ajax/libs/dygraph/1.1.1/dygraph-combined.js"></script>
    </head>

    <body>
    <div id="graphdiv"></div>
    <script type="text/javascript">
      g = new Dygraph(

        // containing div
        document.getElementById("graphdiv"),

        "Date,Temperature\n" +
            {{#results}}
              "{{ts}},{{temperature}}\n" +
            {{/results}}
            "",
            {
      title: 'Temperature',
      ylabel: 'Temperature (C)',
      legend: 'always',
      labelsDivStyles: { 'textAlign': 'right' }
    }
      );
    </script>

    </body>
</html>
